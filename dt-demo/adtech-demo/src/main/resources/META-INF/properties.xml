  </property>
  <property>
    <name>dt.application.adTechDemo.operator.DimensionsComputation.attr.MEMORY_MB</name>
    <value>4048</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.Store.embeddableQueryInfoProvider.topic</name>
    <value>AdTechDemo</value>
  </property>
  <property>
    <name>dt.operator.QueryResult.topic</name>
    <value>AdTechDemoQueryResult</value>
  </property>

