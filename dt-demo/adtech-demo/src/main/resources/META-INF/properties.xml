  </property>
  <property>
    <name>dt.application.adTechDemo.operator.kafkaInput.prop.initialOffset</name>
    <value>LATEST</value>  <!-- broker (NOT zookeeper) address -->
  </property>

  <!-- CsvParser -->
  <property>
    <name>dt.application.adTechDemo.operator.csvParser.attr.MEMORY_MB</name>
    <value>1024</value>
  </property>

  <!-- Store -->
  <property>
    <name>dt.application.adTechDemo.operator.Store.attr.MEMORY_MB</name>
    <value>2048</value>
  </property>
