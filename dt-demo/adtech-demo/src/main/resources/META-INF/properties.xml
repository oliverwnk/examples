<?xml version="1.0"?>
<configuration>

  <property>
    <name>dt.port.*.attr.QUEUE_CAPACITY</name>
    <value>8000</value>
  </property>
  <property>
    <name>dt.attr.CONTAINER_JVM_OPTIONS</name>
    <value>-Xloggc:&lt;LOG_DIR&gt;/gc.log -verbose:gc -XX:+PrintGCDetails -XX:+PrintGCDateStamps</value>
  </property>


  <!-- KafkaDataGenerator -->
  <property>
    <name>dt.application.KafkaDataGenerator.operator.kafkaOutput.prop.properties(bootstrap.servers)</name>
    <value>node27.morado.com:9092</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.kafkaOutput.prop.properties(key.serializer)</name>
    <value>org.apache.kafka.common.serialization.StringSerializer</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.kafkaOutput.prop.properties(value.serializer)</name>
    <value>org.apache.kafka.common.serialization.StringSerializer</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.InputGenerator.numTuplesPerWindow</name>
    <value>45000</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.kafkaOutput.prop.topic</name>
    <value>adtech</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.kafkaOutput.attr.STATELESS</name>
    <value>true</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.InputGenerator.attr.PARTITIONER</name>
    <value>com.datatorrent.common.partitioner.StatelessPartitioner:1</value>
  </property>
  <property>
    <name>dt.application.KafkaDataGenerator.operator.kafkaOutput.port.inputPort.attr.PARTITION_PARALLEL</name>
    <value>true</value>
  </property>
  <!-- adTechDemo Application -->

  <property>
    <name>dt.application.adTechDemo.operator.csvParser.port.out.attr.TUPLE_CLASS</name>
    <value>com.datatorrent.demos.adtech.AdInfo</value>
  </property>

  <!-- FilterOperator -->
  <property>
    <name>dt.application.adTechDemo.operator.filterLocation.port.input.attr.TUPLE_CLASS</name>
    <value>com.datatorrent.demos.adtech.AdInfo</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.filterLocation.prop.condition</name>
    <value>({$}.getLocationID() != 13)</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.filterLocation.prop.expressionFunctionsItem[5]</name>
    <value>org.apache.commons.lang3.BooleanUtils.*</value>
  </property>

  <!-- POJOEnricher -->
  <property>
    <name>dt.application.adTechDemo.operator.Enrich.port.input.attr.TUPLE_CLASS</name>
    <value>com.datatorrent.demos.adtech.AdInfo</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.Enrich.port.output.attr.TUPLE_CLASS</name>
    <value>com.datatorrent.demos.adtech.AdInfo</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.Enrich.prop.store.fileName</name>
    <value>/tmp/enrichMapping.txt</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.Enrich.attr.MEMORY_MB</name>
    <value>4096</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.Enrich.attr.STATELESS</name>
    <value>true</value>
  </property>
  <property>
    <name>dt.application.adTechDemo.operator.Enrich.attr.PARTITIONER</name>
    <value>com.datatorrent.common.partitioner.StatelessPartitioner:1</value>
  </property>

